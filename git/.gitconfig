[user]
  email = sjdonado@icloud.com
  name = Juan Rodriguez Donado

[pull]
  rebase = false

[core]
  editor = nvim

[merge]
  conflictstyle = merge

[diff]
  colorMoved = default

[init]
  defaultBranch = main

[credential]
  helper = store

[push]
  autoSetupRemote = true

[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
