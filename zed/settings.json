{
  "buffer_font_size": 13.0,
  "ui_font_size": 14.0,
  "lsp": {
    "vtsls": {
      "settings": {
        "typescript": {
          "updateImportsOnFileMove": {
            "enabled": "always",
          },
        },
        "javascript": {
          "updateImportsOnFileMove": {
            "enabled": "always",
          },
        },
      },
      "enable_lsp_tasks": true,
    },
  },
  "context_servers": {
    "postgres-context-server": {
      "enabled": true,
      "settings": {
        "database_url": "postgresql://postgres:postgres@localhost:54322/postgres"
      },
    },
  },
  "agent_servers": {
    "claude": {
      "favorite_models": ["sonnet"],
      "default_config_options": {},
      "favorite_config_option_values": {},
    },
  },
  "auto_update": false,
  "git_panel": {
    "tree_view": true,
  },
  "use_system_window_tabs": true,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark",
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "vim_mode": true,
  "soft_wrap": "editor_width",
  "edit_predictions": {
    "provider": "copilot",
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null,
    },
    "enabled_in_text_threads": false,
  },
  "agent": {
    "tool_permissions": {
      "default": "allow"
    },
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-haiku-4.5",
    },
    "default_profile": "write",

    "model_parameters": [],
  },
}
